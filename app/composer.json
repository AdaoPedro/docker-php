{
    "name": "adaopedro/translatethis",
    "description": "A Docker demo app",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Adao Pedro",
            "email": "adaopedro.pro@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "symfony/cache": "^6.1",
        "predis/predis": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.11"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "scripts": {
        "run-phpunit": "vendor/bin/phpunit",
        "run-phpstan": "vendor/bin/phpstan analyse -l max public/index.php src"
    }
}